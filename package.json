    "start": "npm run public && ./node_modules/.bin/webpack-dev-server --open --inline --hot --config ./config/build/webpack.config.js"
  },
  "dependencies": {
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "postcss": "^5.2.13",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.1.0",
    "postcss-load-plugins": "^2.3.0",
    "postcss-loader": "^1.3.0",
    "style-loader": "^0.13.1",
    "vui-theme-oswald-cobelpot": "git+ssh://git@github.com:Cuoore/particle-theme-drmanhattan.git#feature/switch-postcss-webpack2",
    "webpack": "^2.2.1"
  },
  "devDependencies": {
    "webpack-dev-server": "^2.3.0"
  },
  "keywords": [
    "PostCss",
    "Framework",
    "UI",
    "Buttons"
