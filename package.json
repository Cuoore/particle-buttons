{
  "name": "particle-buttons",
  "version": "0.1.0",
  "description": "particle buttons to cuoore framework",
  "author": "@javierlopezdeancos",
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV='production' ./node_modules/.bin/webpack --config ./config/build/webpack.config.js --display-error-details",
